
<%- include('include/_header') %>



  <main>
    <article>

      <!-- 
        - #HERO
      -->

      <section class="hero">
        <div class="container">

          <div class="hero-content">

            <p class="hero-subtitle">doc</p>

            <h1 class="h1 hero-title">
              List <strong>of</strong> Patient medical doc :
              <br />
              <%= user.name %>
            </h1>

            <div class="meta-wrapper">

         
              <div class="ganre-wrapper">
                <a href="#"><%= user.email %></a>

               
              </div>


            </div>

     

          </div>

        </div>
      </section>





      <!-- 
        - #UPCOMING
      -->






      <!-- 
        - #SERVICE
      -->





      <!-- 
        - #TOP RATED
      -->

      <section class="top-rated">
        <div class="container">

          <p class="section-subtitle">Patient Name</p>

          <h2 class="h2 section-title"><%= user.name %></h2>

          <ul class="filter-list">

            <li>
              <button class="filter-btn">Patient Medical Docs</button>
            </li>

            <li>
              <button class="filter-btn">Patient Details</button>
            </li>

            <li>
              <button class="filter-btn">Appointment status</button>
            </li>

            <li>
              <button class="filter-btn">Patient Current Reports</button>
            </li>

          </ul>

          <ul class="movies-list">



       
                <% for(var i=data.length-1; i >=0 ; i--) { %>
                  
                   <li>
                    <div class="movie-card">
      
                      <a href="/doctor-dash/post?id=<%= data[i].id %>">
                        <figure class="card-banner">
                          <img src="<%= data[i].img %>" alt="<%= data[i].img %>">
                        </figure>
                      </a>
      
                      <div class="title-wrapper">
                        <a href="/doctor-dash//post?id=<%= data[i].id %>">
                          <h3 class="card-title"><%= data[i].title %></h3>
                        </a>
      
                        <time datetime="2022"><%= data[i].date.getDate() %>-<%=  data[i].date.getMonth()+1%>- <%=  data[i].date.getYear() - 100%></time>
                      </div>
      
                      <div class="card-meta">
                        <div class="badge badge-outline">IMP</div>
      
                        <div class="duration">
                          <ion-icon name="time-outline"></ion-icon>
      
                          <time datetime="PT122M">lorem</time>
                        </div>
      
                        <div class="rating">
                          <ion-icon name="star"></ion-icon>
      
                          <data>lorem</data>
                        </div>
                      </div>
      
                    </div>
                  </li>
                <% } %>
           
                












          </ul>

        </div>
      </section>





      <!-- 
        - #TV SERIES
      -->






      <!-- 
        - #CTA
      -->



    </article>
  </main>





  <!-- 
    - #FOOTER
  -->







  <!-- 
    - #GO TO TOP
  -->

  <a href="#top" class="go-top" data-go-top>
    <ion-icon name="chevron-up"></ion-icon>
  </a>





  <!-- 
    - custom js link
  -->
  <script src="./assets/js/script.js"></script>

  <!-- 
    - ionicon link
  -->
  <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
  <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>

</body>

</html>